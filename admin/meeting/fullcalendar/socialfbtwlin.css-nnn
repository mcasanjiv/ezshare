.search-box input[type="text"] {
      border: 1px solid !important;
    border-radius: 5px;
    padding: 5px;
    width: 26%;
}
.user-list div {
    background-color: #f4f4f4;
    font-size: 12px;
    line-height: 18px;
    margin-bottom: 4px;
    margin-left: 9px;
    text-align: left;
}
.checkbox-user {
    float: right;
    height: 16px;
    margin-right: 8px;
    width: 16px;
}
.search-result{
float:left;
}
.fblogout {
		background: url("../images/fblogout.png") no-repeat scroll 0 0 transparent;
		height: 21px;
		width: 72px;
	}

.form-action > input {
	
	background: none repeat scroll 0 0 #d40503;
    border: medium none;
    color: #ffffff;
    cursor: pointer;
    font-size: 16px;
    margin: 10px 0;
     padding: 4px;
     border-radius: 4px;
    text-align: center;
}

.user-list img {
    border-radius: 7px;
}