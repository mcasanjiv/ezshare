#dhtmltooltip{
position: absolute;
color:#ffffff;
font-size: 12px;
font-weight: normal;
left: -300px;
padding: 5px;
background-color: #333;
visibility: hidden;
z-index: 100;
border-radius:5px;

/*Remove below line to remove shadow. Below line should always appear last within this CSS*/
filter: progid:DXImageTransform.Microsoft.Shadow(color=gray,direction=135);
}

#dhtmlpointer{
position:absolute;
left: -300px;
z-index: 101;
visibility: hidden;
}

