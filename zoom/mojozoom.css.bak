
div.mojozoom_marker {
	position : absolute;
	cursor : crosshair; 
}

div.mojozoom_border {
	position : absolute;
	left:0px;
	top:0px;

	border : 0px solid black;

	width:100%;
	height:100%;

}

div.mojozoom_fill {
	position : absolute;
	left:0px;
	top:0px;
	width:100%;
	height:100%;

	background-color : white;

	opacity : 0.1;
	-moz-opacity : 0.1;
	-khtml-opacity : 0.1;
	filter:alpha(opacity=10);




}


img.mojozoom_img {
	padding : 0px;
	border: 0px;
}

div.mojozoom_imgctr {
	background-color : #000000;
	position : absolute;
	margin-left : 40px;
	border : 1px solid #666;
	padding:10px;
}

